{
  "name": "hoouse-api-v1",
  "version": "1.0.0",
  "private": true,
  "description": "API NodeJS HOOUSE",
  "main": "dist/server.js",
  "scripts": {
    "test": "./node_modules/.bin/_mocha --recursive --exit ./dist/test/*.test.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/server.ts",
    "start": "node --trace-warnings dist/server.js",
    "build": "tsc -p .",
    "coverage": "nyc yarn test",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm"
  },
  "keywords": [],
  "author": "Adrien MAILLARD, Allan BUSI",
  "dependencies": {
    "argon2": "^0.28.2",
    "axios": "0.21.4",
    "child-process-async": "1.0.1",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-status-monitor": "1.3.4",
    "helmet": "6.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "2.2.2",
    "mysql": "2.18.1",
    "node-cache": "5.1.2",
    "node-cookie": "2.1.2",
    "nodemailer": "^6.6.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "pino": "8.6.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
